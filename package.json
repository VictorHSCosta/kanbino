{
  "name": "kanbino",
  "version": "1.0.0",
  "description": "A professional Node.js project with TypeScript and comprehensive testing suite",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:frontend": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc",
    "build:frontend": "tsc --project frontend/tsconfig.json && vite build",
    "build:all": "npm run build && npm run build:frontend",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "preview:frontend": "vite preview",
    "build:css": "postcss src/styles/input.css -o src/public/css/output.css",
    "build:css:watch": "postcss src/styles/input.css -o src/public/css/output.css --watch",
    "build:css:prod": "postcss src/styles/input.css -o src/public/css/output.css --env production",
    "lint": "eslint src tests frontend --ext .ts,.tsx",
    "lint:fix": "eslint src tests frontend --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"frontend/**/*.ts\" \"frontend/**/*.tsx\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"frontend/**/*.ts\" \"frontend/**/*.tsx\"",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "tsc --noEmit --project frontend/tsconfig.json",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorHSCosta/kanbino.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "react",
    "vite",
    "jest",
    "testing",
    "professional",
    "boilerplate",
    "fullstack"
  ],
  "author": "Kanbino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VictorHSCosta/kanbino/issues"
  },
  "homepage": "https://github.com/VictorHSCosta/kanbino#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "express-session": "^1.18.1",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/passport": "^1.0.16",
    "@types/express-session": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.0.0"
  }
}
